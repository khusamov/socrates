{
  "name": "socrates",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --print-label --parallel server:tsc:w server:nodemon client:start",
    "client:start": "react-app-rewired start --scripts-version react-scripts-ts",
    "client:build": "react-app-rewired build --scripts-version react-scripts-ts",
    "client:test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "server:tsc:w": "tsc -w -p server",
    "server:nodemon": "nodemon server/dist"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "camelcase": "^5.0.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.4",
    "http-status": "^1.2.0",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "path": "^0.12.7",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-scripts-ts": "2.17.0",
    "sqlite": "^2.9.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/classnames": "^2.2.6",
    "@types/express": "^4.16.0",
    "@types/http-status": "^0.2.30",
    "@types/jest": "^23.3.2",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.31",
    "@types/koa__cors": "^2.2.3",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/uuid": "^3.4.4",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "react-app-rewire-scss": "^1.0.2",
    "react-app-rewired": "^1.6.2",
    "reflect-metadata": "^0.1.12",
    "typescript": "^3.0.3"
  }
}
