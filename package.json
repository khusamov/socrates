{
  "name": "socrates",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --print-label --parallel server:tsc:w server:nodemon client:start",
    "client:start": "react-app-rewired start --scripts-version react-scripts-ts",
    "client:build": "react-app-rewired build --scripts-version react-scripts-ts",
    "client:test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "server:tsc:w": "tsc -w -p server",
    "server:nodemon": "nodemon server/dist"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "camelcase": "^5.0.0",
    "cors": "^2.8.4",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-router": "^7.4.0",
    "path": "^0.12.7",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts-ts": "2.17.0",
    "sqlite": "^2.9.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.31",
    "@types/koa__cors": "^2.2.3",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/uuid": "^3.4.3",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.3",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "react-app-rewire-scss": "^1.0.2",
    "react-app-rewired": "^1.5.2",
    "typescript": "^3.0.1"
  }
}
